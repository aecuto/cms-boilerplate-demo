{
  "name": "hapi-boilerplate",
  "version": "0.1.0",
  "description": "a starter service project for 20scoops cnx ",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "start": "node ./src/index.js",
    "dev": "nodemon src/index.js",
    "dev:docker": "nodemon -L src/index.js",
    "db:seed": "SEEDTYPE=UP node ./src/database/seeders/index.js && exit 1",
    "db:seed:down": "SEEDTYPE=DOWN node ./src/database/seeders/index.js && exit 1",
    "lint": "eslint src --color",
    "format": "eslint src --fix",
    "prettier": "prettier --write 'src/**/*.js'",
    "prettier:check": "prettier --list-different 'src/**/*.js'",
    "prettier-changefiles": "pretty-quick --staged",
    "precommit": "yarn run prettier-changefiles",
    "test:watch": "JWT_SECRET=test jest --coverage --watch",
    "test": "JWT_SECRET=test jest --coverage",
    "check-vulnerabilities": "yarn retire --outputformat json --outputpath artifacts/result-vulnerabilities.json",
    "logs": "docker-compose logs -f --tail=100 service",
    "up": "docker-compose up -d",
    "down": "docker-compose down",
    "docker": "docker-compose exec service"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier-changefiles"
    }
  },
  "_moduleAliases": {
    "@root": "src",
    "@models": "src/models",
    "@helpers": "src/helpers",
    "@query-builder": "src/query-builer",
    "@dao": "src/dao",
    "@constant": "src/constant"
  },
  "_moduleDirectories": [
    "src"
  ],
  "keywords": [
    "service",
    "20scoops",
    "api-starter",
    "nodejs-mongodb",
    "hapi-mongoose"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "20scoops-localizer": "^1.2.6",
    "@hapi/boom": "^9.0.0",
    "@hapi/hapi": "^19.0.5",
    "@hapi/inert": "^6.0.1",
    "@hapi/joi": "^17.1.0",
    "@hapi/scooter": "^6.0.0",
    "@hapi/vision": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "blankie": "^4.1.1",
    "dotenv": "^8.1.0",
    "geoip-lite": "^1.3.8",
    "handlebars": "^4.1.2",
    "hapi-auth-jwt2": "^8.8.1",
    "hapi-i18n": "^3.0.0",
    "hapi-rate-limit": "^4.1.0",
    "hapi-response-time": "^2.1.0",
    "hapi-sentry": "^3.0.0",
    "hapi-swagger": "^12.0.0",
    "joi-objectid": "^3.0.1",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "mask-json": "^1.0.3",
    "module-alias": "^2.2.2",
    "moment": "^2.24.0",
    "mongoose": "^5.8.10",
    "mongoose-aggregate-paginate-v2": "^1.0.3",
    "mongoose-delete": "^0.5.1",
    "node-cron": "^2.0.3",
    "request-ip": "^2.1.3",
    "vision": "^5.4.4",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.14.3",
    "pretty-quick": "^2.0.1",
    "retire": "^2.0.3",
    "shipit-cli": "^5.1.0",
    "shipit-deploy": "^5.1.0",
    "shipit-shared": "^4.4.2"
  },
  "engines": {
    "node": ">=12.14.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/20Scoops-CNX/hapi-mongoose-demo.git"
  },
  "bugs": {
    "url": "https://github.com/20Scoops-CNX/hapi-mongoose-demo/issues"
  },
  "homepage": "https://github.com/20Scoops-CNX/hapi-mongoose-demo#readme"
}
